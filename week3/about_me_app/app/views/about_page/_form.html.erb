<div class="container mt-3 pt-3">
    Hi, Please fill the form to access the full website!
    <%= bootstrap_form_with url: "/about_page", method: :post do |form|%>
<div class="container mt-3 pt-3" >
<%=form.text_field :name %>
</div>
<div class="container mt-3 pt-3">
<%=form.text_field :email%>
</div>
<div class="container mt-3 pt-3">
<%=form.submit "Go to About-Me" %>
</div>
<% end %>
</div>

